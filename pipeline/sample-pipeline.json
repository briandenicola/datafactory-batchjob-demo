{
	"name": "sample-pipeline",
	"properties": {
		"activities": [
			{
				"name": "Push to Service Bus",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://bjdsb-f81dfea.servicebus.windows.net/batchjob/message",
					"connectVia": {
						"referenceName": "AutoResolveIntegrationRuntime",
						"type": "IntegrationRuntimeReference"
					},
					"method": "POST",
					"headers": {
						"CustomHeader-Version": "1.0",
						"Content-Type": "Application/json",
						"BrokerProperties": {
							"value": "concat('{\\\"CorrelationId\\\": \\\"',pipeline().RunId,'\\\"}')",
							"type": "Expression"
						}
					},
					"body": {
						"value": "{\n  \"message\": {\n      \"file\": @concat(@triggerBody().folderPath, \"/\", @triggerBody().fileName)\n   }\n}",
						"type": "Expression"
					},
					"authentication": {
						"resource": "https://servicebus.azure.net",
						"credential": {
							"referenceName": "bjddf-f81dfea-identity",
							"type": "CredentialReference"
						}
					}
				}
			}
		],
		"annotations": []
	}
}